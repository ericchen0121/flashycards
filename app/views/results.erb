<div class="container">
	<div id="results">
		<h3>Round Stats</h3>
		<div class="list_everything">
      <ul>
        <li>Deck: <%= @results[:deck_name] %></li>
  		  <li>Round# <%= @results[:round] %></li>
  		  <li>Total Cards: <%= @results[:total] %></li>
  		  <li>Correct: <%= @results[:correct] %> (<%= @results[:percent_correct] %>%)</li>
  		  <li>Incorrect: <%= @results[:incorrect] %> (<%= @results[:percent_incorrect] %>%)</li>
      </ul>
		</div>
    <div>
    	<p><a href= '/results/all' >View all results</a></p>
		</div>
	</div>
</div>
